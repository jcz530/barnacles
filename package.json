{
  "name": "barnacles",
  "productName": "Barnacles",
  "version": "0.2.9",
  "description": "App for developers",
  "main": "dist/main/main.js",
  "type": "module",
  "bin": {
    "barnacles": "./dist/cli/index.js",
    "barnacles-dev": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:main": "cross-env NODE_ENV=development vite build --config vite.main.config.ts --watch",
    "dev:preload": "cross-env NODE_ENV=development vite build --config vite.preload.config.ts --watch",
    "dev:renderer": "cross-env NODE_ENV=development vite --config vite.renderer.config.mjs",
    "dev:electron": "cross-env NODE_ENV=development nodemon --watch dist/main --delay 1000ms --exec \"electron dist/main/main.js\"",
    "dev:cli": "vite build --mode development --config vite.cli.config.ts && cross-env NODE_ENV=development node dist/cli/index.js",
    "build": "cross-env NODE_ENV=production npm run build:main && npm run build:preload && npm run build:renderer && npm run build:cli",
    "build:main": "cross-env NODE_ENV=production vite build --config vite.main.config.ts",
    "build:preload": "cross-env NODE_ENV=production vite build --config vite.preload.config.ts",
    "build:renderer": "cross-env NODE_ENV=production vite build --config vite.renderer.config.mjs",
    "build:cli": "cross-env NODE_ENV=production vite build --config vite.cli.config.ts && chmod +x dist/cli/index.js",
    "start": "cross-env NODE_ENV=production npm run build && electron dist/main/main.js",
    "dist": "./scripts/build-production.sh",
    "dist:mac": "./scripts/build-production.sh --mac",
    "dist:win": "./scripts/build-production.sh --win",
    "dist:linux": "./scripts/build-production.sh --linux",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,css}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "rebuild": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": {
    "name": "Joe Czubiak",
    "url": "https://joeczubiak.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-query": "^5.90.2",
    "@tanstack/vue-table": "^8.21.3",
    "@types/culori": "^4.0.1",
    "@types/dompurify": "^3.0.5",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.5.2",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "dompurify": "^3.2.7",
    "drizzle-kit": "^0.31.5",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.0",
    "fuse.js": "^7.1.0",
    "lucide-vue-next": "^0.544.0",
    "marked": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "reka-ui": "^2.6.0",
    "shadcn-vue": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0-alpha.6",
    "@hono/node-server": "^1.19.4",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "culori": "^4.0.2",
    "dayjs": "^1.11.18",
    "drizzle-orm": "^0.44.5",
    "electron-context-menu": "^4.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "exifreader": "^4.32.0",
    "hono": "^4.9.8",
    "node-pty": "^1.0.0",
    "piexifjs": "^1.0.6",
    "shiki": "^3.14.0",
    "vue-sonner": "^2.0.9",
    "ws": "^8.18.3"
  }
}
